<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeCold"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeCold"]/biomes</xpath>
          <value>
            <li>Permafrost</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
          <value>
            <li>Woodland</li>
            <li>Grassland</li>
            <li>MoistDeciduousForest</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]/biomes</xpath>
          <value>
            <li>Savanna</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
